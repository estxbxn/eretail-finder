{
  "manifest_version": 3,
  "name": "Retail Class Detector",
  "version": "1.0",
  "description": "Dynamically detects specific classes for various retailers.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ],
  "icons": {
    "48": "assets/icon.png"
  },
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"]
}